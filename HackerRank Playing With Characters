#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
    char ch;
    char s[100];
    char sen[100];
    
    // Read the character
    scanf("%c", &ch);
    
    // Read the string (no spaces)
    scanf("%s", s);
    
    // Clear the leftover newline before reading the sentence
    scanf("\n");
    
    // Read the full sentence
    scanf("%[^\n]%*c", sen);

    // Output
    printf("%c\n", ch);
    printf("%s\n", s);
    printf("%s\n", sen);
    
    return 0;
}
