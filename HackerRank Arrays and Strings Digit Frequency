#include <stdio.h>
#include <string.h>

int main() {
    char s[10000];
    int freq[10] = {0};

    // Read the input string
    scanf("%s", s);

    // Count digit frequencies
    for (int i = 0; s[i] != '\0'; i++) {
        if (s[i] >= '0' && s[i] <= '9') {
            freq[s[i] - '0']++;
        }
    }

    // Print the frequencies
    for (int i = 0; i < 10; i++) {
        printf("%d ", freq[i]);
    }

    return 0;
}
